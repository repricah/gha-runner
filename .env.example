# GitHub repository URL (required)
REPO_URL=https://github.com/your-org/your-repo

# Runner registration token (required - generate from repo settings)
# Get from: https://github.com/your-org/your-repo/settings/actions/runners
RUNNER_TOKEN=dummy_token_for_validation

# Runner labels (optional)
RUNNER_LABELS=self-hosted,dind,buildkit

# Base runner name (optional - will be suffixed with numbers)
RUNNER_NAME_PREFIX=gha-runner

# Number of runners to start (optional, default: 1)
RUNNER_COUNT=3

# Resource limits (optional)
RUNNER_MEMORY_LIMIT=2g
RUNNER_CPU_LIMIT=1.0
